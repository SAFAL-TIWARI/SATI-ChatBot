        }
    ],
    "prefer_related_applications": false,
    "edge_side_panel": {
        "preferred_width": 400
    },
    "launch_handler": {
        "client_mode": "navigate-existing"
    },
    "handle_links": "preferred",
    "capture_links": "existing-client-navigate",
    "protocol_handlers": [
        {
            "protocol": "web+sati",
            "url": "/?action=%s"
        }
    ],
    "file_handlers": [
        {
            "action": "/",
            "accept": {
                "text/plain": [
                    ".txt"
                ],
                "application/pdf": [
                    ".pdf"
                ]
            }
        }
    ],
    "share_target": {
        "action": "/",
        "method": "GET",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url"
        }
    }
